<%= form_for(@project) do |f| %>
<p><%= f.label :name %>
<%= f.text_field :name %>
</p>
<p><%= f.label :description %>
<%= f.text_field :description %> (i.e. novel, short story, research paper, essay)</p>
<p><%= f.label :summary %></p>
<p><%= f.text_area(:summary, size: "50x15")%></p>

<%= f.submit %>
<% end %>

